package mypackage;

public class SmallLarge {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr={12,34,14,33,22,11,19,45,61};
		int s,l;
		s=l=arr[0];
		for(int i:arr){
			if(i>l){
				l=i;
			}
			else if(i<s){
				s=i;
			}
		}
		System.out.println("smallest number"+s);
		System.out.println("Largest number"+l);

	}

}
